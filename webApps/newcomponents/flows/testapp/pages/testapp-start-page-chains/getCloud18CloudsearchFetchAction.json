{
  "root": "callRestEndpoint1",
  "actions": {
    "callRestEndpoint1": {
      "module": "vb/action/builtin/restAction",
      "parameters": {
        "requestType": "json",
        "endpoint": "test/getCloud18Cloudsearch",
        "responseType": "getCloud18Cloudsearch",
        "hookHandler": "{{ $variables.configuration.hookHandler }}"
      },
      "outcomes": {
        "success": "return1"
      }
    },
    "return1": {
      "module": "vb/action/builtin/returnAction",
      "parameters": {
        "outcome": "success",
        "payload": "{{ $chain.results.callRestEndpoint1 }}"
      }
    }
  },
  "variables": {
    "configuration": {
      "type": {
        "hookHandler": "vb/RestHookHandler"
      },
      "input": "fromCaller",
      "required": true
    }
  }
}